<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane id="root" hgap="8.0" maxHeight="-Infinity" styleClass="grid" vgap="8.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.qenherkhopeshef.jsesh.codeDumper.FXMLController">
   <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="10.0" />
      <ColumnConstraints halignment="LEFT" hgrow="NEVER" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <TextField fx:id="sourceFolderField" editable="false" prefHeight="27.0" prefWidth="290.0" />
      <TextField fx:id="destFolderField" editable="false" GridPane.rowIndex="1" />
      <Button mnemonicParsing="false" onAction="#changeSourceAction" text="choose source folder" GridPane.columnIndex="1" GridPane.hgrow="NEVER" />
      <Button alignment="TOP_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#changeDestAction" text="choose dest folder" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="1" GridPane.vgrow="NEVER" />
      <Button mnemonicParsing="false" onAction="#extractCodesAction" text="extract codes" GridPane.rowIndex="3">
         <GridPane.margin>
            <Insets top="10.0" />
         </GridPane.margin></Button>
      <Label fx:id="messageField" prefHeight="23.0" prefWidth="293.0" GridPane.rowIndex="2" />
   </children>
   <opaqueInsets>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </opaqueInsets>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</GridPane>
